{
  "$schema": "./node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "6.4.0",
    "generators": {
      "OSIDB": {
        "generatorName": "typescript-fetch",
        "output": "src/generated-client/osidb",
        "inputSpec": "openapi-osidb.yml",
        "additionalProperties": {
          "supportsES6": "true",
          "withoutRuntimeChecks": true
        }
      },
      "AEGIS-AI": {
        "generatorName": "typescript-fetch",
        "output": "src/generated-client/aegis-ai",
        "inputSpec": "openapi-aegis-ai.yml",
        "additionalProperties": {
          "supportsES6": "true",
          "withoutRuntimeChecks": true
        }
      }
    }
  }
}