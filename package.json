{
  "name": "osim",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/RedHatProductSecurity/osim.git"
  },
  "license": "MIT",
  "private": false,
  "scripts": {
    "dev": "scripts/dev-osim.sh",
    "dev-server": "vite",
    "build": "run-p type-check lint \"build-only {@}\" \"test:unit:nowatch {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:unit:nowatch": "vitest run",
    "test:e2e": "behave features",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "vibe-check": "npx vite-bundle-visualizer",
    "generate-openapi-client": "openapi-generator-cli generate -i osidb-3.4.0.oas3.json --output src/generated-client -g typescript-axios",
    "mock-osidb-server": "prism mock osidb-3.0.0.oas3.json",
    "lint": "yarn lint-style && yarn lint-ts-vue",
    "lint-ts-vue": "eslint . --ext .js,.ts,.vue",
    "lint-style": "stylelint '**/*.{vue,css,scss}'"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "@vee-validate/zod": "^4.11.1",
    "@vueuse/core": "^10.4.1",
    "axios": "^1.4.0",
    "bootstrap": "^5.3.2",
    "bootstrap-icons": "^1.10.5",
    "jwt-decode": "^3.1.2",
    "luxon": "^3.4.3",
    "maska": "^2.1.9",
    "moment": "^2.29.4",
    "pinia": "^2.1.4",
    "sanitize-html": "^2.12.1",
    "vee-validate": "^4.11.1",
    "vue": "^3.4.19",
    "vue-imask": "^7.1.2",
    "vue-router": "^4.2.4",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.6.0",
    "@pinia/testing": "^0.1.3",
    "@stoplight/prism-cli": "^5.0.1",
    "@stylistic/eslint-plugin-js": "^1.6.3",
    "@tsconfig/node18": "^18.2.2",
    "@types/jsdom": "^21.1.3",
    "@types/luxon": "^3.3.2",
    "@types/node": "^20.4.0",
    "@types/sanitize-html": "^2.11.0",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "@vitejs/plugin-basic-ssl": "^1.0.1",
    "@vitejs/plugin-vue": "^4.3.4",
    "@vue/test-utils": "^2.4.1",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.21.1",
    "jsdom": "^22.1.0",
    "msw": "^2.1.2",
    "npm-run-all2": "^6.0.6",
    "postcss-html": "^1.6.0",
    "sass": "^1.63.6",
    "sass-loader": "^13.3.2",
    "stylelint": "^16.2.1",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-config-standard-scss": "^13.0.0",
    "stylelint-scss": "^6.1.0",
    "typescript": "~5.2.0",
    "vite": "^4.4.9",
    "vite-bundle-visualizer": "^1.0.0",
    "vitest": "^0.34.4",
    "vue-tsc": "^1.8.11"
  }
}
